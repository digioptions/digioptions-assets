{
  "name": "digioptions-assets",
  "version": "0.3.0",
  "description": "Common base assets like graphics, logos and favicons for building web apps",
  "type": "module",
  "homepage": "https://github.com/digioptions/digioptions-assets",
  "license": "All rights reserved.",
  "author": {
    "name": "Ulf Bartel",
    "email": "elastic.code@gmail.com"
  },
  "scripts": {
    "build": "rm -rf dist/cjs dist/cjs-react dist/mjs dist/mjs-react && tsc -p tsconfig-mjs.json && tsc -p tsconfig-mjs-react.json && tsc -p tsconfig-cjs.json && tsc -p tsconfig-cjs-react.json",
    "eslint": "eslint *.cjs src/*.js tools/*.js",
    "watch-cjs": "tsc -p tsconfig-cjs.json --watch",
    "watch-cjs-react": "tsc -p tsconfig-cjs.json --watch",
    "watch-mjs": "tsc -p tsconfig-mjs.json --watch",
    "watch-mjs-react": "tsc -p tsconfig-mjs.json --watch"
  },
  "files": [
    "css",
    "dist",
    "favicon",
    "icons",
    "img",
    "LICENSE",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/digioptions/digioptions-assets.git"
  },
  "keywords": [],
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/eslint-parser": "^7.16.3",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "@types/node": "^17.0.8",
    "eslint": "^8.4.0",
    "eslint-plugin-react": "^7.23.0",
    "react": "~17.0.0",
    "react-dom": "~17.0.0",
    "sharp": "^0.29.3",
    "typescript": "^4.5.4"
  }
}
